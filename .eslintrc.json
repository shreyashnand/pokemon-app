{
    "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
    "rules": {
      "indent": ["error", 2], 
      "comma-dangle": ["error", "always-multiline"],
      "space-before-function-paren": ["error", "always"], 
      "object-curly-spacing": ["error", "always"], 
      "array-bracket-spacing": ["error", "never"], 
      "semi": ["error", "always"], 
      "quotes": ["error", "double"]
    }
  }
  